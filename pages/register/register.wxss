/* pages/register/register.wxss */
input {
  outline-style: none ; /*取消外边框*/
  border: 1px solid #ccc; /*设置灰色 1px的边框 */
  border-radius: 3px; /*设置边框圆角效果*/
  padding: 7px 8px; /*高度*/
  width: 120%; /*宽度*/
  font-size: 14px; /*字体大小*/
}

.register {
  margin-top: 30px;
}

.ms-content-box{
  background: #fff;
  width: 100%;
  position: relative;
 }

.top-selected{
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
 }

 .ms-text{
  font-size: 30rpx;
  flex: 1;
 }
